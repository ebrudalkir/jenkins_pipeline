node{
    stage('Build'){
        sh """
        docker build . -t ebrudalkir/deneme-helloworld:${tag}
        """
    }
    stage('show images'){
        sh """
        docker images
        """
    }
    stage('push Dockerhub'){
        sh """
        docker push ebrudalkir/deneme-helloworld:${tag}
        """
    }
}
